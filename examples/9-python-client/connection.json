{
    "id": "test-connection",
    "name": "Test Connection",
    "connectionType": "mqtt",
    "connectionStatus": "open",
    "uri": "tcp://host.docker.internal:1883",
    "failoverEnabled": true,
    "specificConfig": {
        "clientId": "ditto-client-test",
        "cleanSession": true,
        "keepAlive": 60
    },
    "sources": [
        {
            "addresses": ["test/topic"],
            "authorizationContext": ["nginx:ditto"],
            "qos": 0
        }
    ],
    "targets": [
        {
            "address": "test/response",
            "authorizationContext": ["nginx:ditto"],
            "qos": 0
        }
    ]
}
